{
  "name": "@nextpay/orix-types",
  "version": "1.0.1",
  "description": "Shared TypeScript types, interfaces, enums, and constants for Orix projects",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./interfaces": {
      "import": "./dist/interfaces/index.js",
      "types": "./dist/interfaces/index.d.ts"
    },
    "./enums": {
      "import": "./dist/enums/index.js", 
      "types": "./dist/enums/index.d.ts"
    },
    "./interfaces/auth": {
      "import": "./dist/interfaces/user/index.js",
      "types": "./dist/interfaces/user/index.d.ts"
    },
    "./interfaces/order": {
      "import": "./dist/interfaces/order/index.js",
      "types": "./dist/interfaces/order/index.d.ts"
    },
    "./interfaces/wallet": {
      "import": "./dist/interfaces/wallet/index.js",
      "types": "./dist/interfaces/wallet/index.d.ts"
    },
    "./interfaces/bffweb": {
      "import": "./dist/interfaces/bffweb/index.js",
      "types": "./dist/interfaces/bffweb/index.d.ts"
    },
    "./enums/auth": {
      "import": "./dist/enums/user/index.js",
      "types": "./dist/enums/user/index.d.ts"
    },
    "./enums/order": {
      "import": "./dist/enums/order/index.js",
      "types": "./dist/enums/order/index.d.ts"
    },
    "./enums/wallet": {
      "import": "./dist/enums/wallet/index.js",
      "types": "./dist/enums/wallet/index.d.ts"
    },
    "./enums/bffweb": {
      "import": "./dist/enums/bffweb/index.js",
      "types": "./dist/enums/bffweb/index.d.ts"
    },
    "./constants": {
      "import": "./dist/constants/index.js",
      "types": "./dist/constants/index.d.ts"
    },
    "./constants/order": {
      "import": "./dist/constants/order/order.constant.js",
      "types": "./dist/constants/order/order.constant.d.ts"
    },
    "./constants/wallet": {
      "import": "./dist/constants/wallet/transaction.constant.js",
      "types": "./dist/constants/wallet/transaction.constant.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    }

    
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build-types": "tsc",
    "dev-types": "tsc --watch",
    "clean": "rimraf dist",
    "prepare": "pnpm run clean && pnpm run build-all",
    "typecheck": "tsc --noEmit",
    "release:patch": "pnpm version patch && git push && git push --tags",
    "release:minor": "pnpm version minor && git push && git push --tags",
    "release:major": "pnpm version major && git push && git push --tags",
    "version": "pnpm run build-types && git add dist/",
    "publish:git": "git add . && git commit -m \"update types\" && git push origin main"
    
  },
  "keywords": [
    "typescript",
    "types",
    "interfaces",
    "enums",
    "orix"
  ],
  "author": "Orix Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.saobang.vn/nextpay1/orix/fe/web/orix-types.git"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "rimraf": "^6.0.1",
    "typescript": "~5.8.3",
    "vite": "^7.1.2"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "dayjs": "^1.11.18"
  }
}
